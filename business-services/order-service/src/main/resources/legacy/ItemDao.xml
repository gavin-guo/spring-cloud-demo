<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.gavin.dao.ItemDao">
    <insert id="create" parameterType="com.gavin.model.domain.order.Item">
        INSERT INTO item (
            product_id,
            quantity,
            order_id
        ) VALUES (
            ${productId},
            ${quantity},
            ${orderId}
        )
    </insert>

</mapper>